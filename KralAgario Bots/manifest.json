{
  "update_url": "https://clients2.google.com/",
  "author": "AgarThix",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "thixbots.tk",
    "default_icon": "img/btn/icon128.png"
  },
  "content_scripts": [{
    "js": ["inject.js"],
    "matches": ["*://kralagario.com/", "*://kralagario.com/index.html", "*://kralagario.com/?ip=*"],
    "run_at": "document_start"
  }],
  "description": "ThxBots.tk | Creator: AgarThix |",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "16": "img/icon16.png"
  },
  "manifest_version": 2,
  "name": "kralagario Bots by AgarThix",
  "permissions": ["*://kralagario.com/*", "webRequest", "webRequestBlocking"],
  "short_name": "Agar.io+",
  "version": "1.8.0",
  "web_accessible_resources": ["html/*", "img/*"]
}